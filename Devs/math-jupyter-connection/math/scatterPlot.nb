Cell[TextData[{
 StyleBox["Define the mathematical function that will be evaluated",
  FontWeight->"Bold"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{"a", " ", 
      RowBox[{"1", "/", "x"}]}], "-", 
