  RowBox[{
   RowBox[{"setlabel", "[", "params_", "]"}], ":=", 
   RowBox[{
    RowBox[{"StringTemplate", "[", "\"\<a=`` | b=``\>\"", "]"}], "[", 
    RowBox[{
     RowBox[{"params", "[", 
      RowBox[{"[", "1", "]"}], "]"}], ",", 
     RowBox[{"params", "[", 
      RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
