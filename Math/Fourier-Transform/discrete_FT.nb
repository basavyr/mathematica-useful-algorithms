  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"xdata", "[", "N_", "]"}], ":=", 
   RowBox[{"mapper", "[", 
    RowBox[{"rad", ",", 
     RowBox[{"xdataRaw", "[", "N", "]"}]}], "]"}]}], ";"}]}], "Input",
