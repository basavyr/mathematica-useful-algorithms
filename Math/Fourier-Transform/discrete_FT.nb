0d1f1550ce71"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"mapper", "[", 
    RowBox[{"function_", ",", "data_"}], "]"}], ":=", 
   RowBox[{"Map", "[", 
    RowBox[{"function", ",", "data"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
