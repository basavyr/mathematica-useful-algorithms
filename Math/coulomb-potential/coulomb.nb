Cell[CellGroupData[{

Cell[TextData[{
 "The protons interact via the Coulomb force.\nThe Coulomb potential inside \
and outside a homogeneous charge of radius ",
 Cell[BoxData[
  FormBox[
   RowBox[{"R", "=", 
    RowBox[{
     SubscriptBox["r", "0"], 
     SuperscriptBox["A", 
      RowBox[{"1", "/", "3"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "f6c1ca1b-edd8-4d46-88be-88833cd51799"],
 " is as follows:\n"
}], "Subsection",
