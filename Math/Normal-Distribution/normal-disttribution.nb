Cell["Perform AUC calculations for a normal distribution", "Section",
 CellChangeTimes->{{3.8416336713521967`*^9, 
  3.841633689521729*^9}},ExpressionUUID->"ee6bd1b7-1f69-4dbc-907f-\
2a1b4cb72948"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"fx", "[", "x_", "]"}], ":=", 
   RowBox[{
    FractionBox["1", 
     SqrtBox[
      RowBox[{"2", "\[Pi]"}]]], 
    RowBox[{"Exp", "[", 
     RowBox[{
      FractionBox[
       RowBox[{"-", "1"}], "2"], 
      SuperscriptBox[
       RowBox[{"(", "x", ")"}], "2"]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"fnormal", "[", 
    RowBox[{"x_", ",", "mu_", ",", "sigma_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["1", "sigma"], "*", 
    RowBox[{"fx", "[", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"x", "-", "mu"}], ")"}], "/", "sigma"}], "]"}]}]}], 
  ";"}]}], "Input",
 CellChangeTimes->{{3.841633864232049*^9, 3.841634048530239*^9}},
 CellLabel->"In[1]:=",ExpressionUUID->"fd088463-1730-47aa-9cfd-2a0707e34446"],

