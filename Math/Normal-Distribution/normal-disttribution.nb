   RowBox[{"{", 
    RowBox[{"x", ",", 
     RowBox[{"fnormal", "[", 
      RowBox[{"x", ",", "mu", ",", "sigma"}], "]"}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"distribution", "[", 
    RowBox[{"mu_", ",", "sigma_", ",", "delta_"}], "]"}], ":=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"normalTuple", "[", 
      RowBox[{"arg", ",", "mu", ",", "sigma"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"arg", ",", 
       RowBox[{"-", "delta"}], ",", "delta", ",", "0.1"}], "}"}]}], "]"}]}], 
  ";"}]}], "Input",
