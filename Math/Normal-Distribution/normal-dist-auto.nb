Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"fNormal", "[", 
    RowBox[{"x_", ",", "xargs_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"xargs", "[", 
      RowBox[{"[", "1", "]"}], "]"}]], "*", 
    FractionBox["1", 
     RowBox[{"\[Sqrt]", 
      RowBox[{"(", 
       RowBox[{"2", "\[Pi]"}], ")"}]}]], 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["1", "2"]}], "*", 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{"x", "-", 
          RowBox[{"xargs", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], 
         RowBox[{"xargs", "[", 
          RowBox[{"[", "1", "]"}], "]"}]], ")"}], "2"]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"normalData", "[", 
    RowBox[{"xargs_", ",", "xmin_", ",", "xmax_"}], "]"}], ":=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"fNormal", "[", 
        RowBox[{"x", ",", "xargs"}], "]"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", "xmin", ",", "xmax", ",", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"xmax", "-", "xmin"}], ")"}], "/", "100"}]}], "}"}]}], 
    "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.842753715823134*^9, 3.842753811226799*^9}, {
