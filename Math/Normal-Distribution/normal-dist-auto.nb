Cell["Generate the normal distribution", "Subsection",
 CellChangeTimes->{{3.842753868792107*^9, 
  3.842753879258897*^9}},ExpressionUUID->"5aa76e40-7be0-42ff-8bd0-\
19350116909c"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"fNormal", "[", 
    RowBox[{"x_", ",", "xargs_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"xargs", "[", 
      RowBox[{"[", "1", "]"}], "]"}]], "*", 
    FractionBox["1", 
     RowBox[{"\[Sqrt]", 
      RowBox[{"(", 
       RowBox[{"2", "\[Pi]"}], ")"}]}]], 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["1", "2"]}], "*", 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{"x", "-", 
          RowBox[{"xargs", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], 
         RowBox[{"xargs", "[", 
          RowBox[{"[", "1", "]"}], "]"}]], ")"}], "2"]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"normalData", "[", 
    RowBox[{"xargs_", ",", "xmin_", ",", "xmax_"}], "]"}], ":=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"fNormal", "[", 
        RowBox[{"x", ",", "xargs"}], "]"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", "xmin", ",", "xmax", ",", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"xmax", "-", "xmin"}], ")"}], "/", "100"}]}], "}"}]}], 
    "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.842753715823134*^9, 3.842753811226799*^9}, {
  3.842753844586774*^9, 3.842753847215857*^9}},
 CellLabel->"In[45]:=",ExpressionUUID->"e6c314c9-60a4-4a58-bffc-039533d41d1e"]
}, Open  ]],

