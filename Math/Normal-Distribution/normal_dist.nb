     RowBox[{"xargs", "[", 
      RowBox[{"[", "1", "]"}], "]"}]], "*", 
    FractionBox["1", 
     RowBox[{"\[Sqrt]", 
      RowBox[{"(", 
       RowBox[{"2", "\[Pi]"}], ")"}]}]], 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["1", "2"]}], "*", 
      SuperscriptBox[
       RowBox[{"(", 
