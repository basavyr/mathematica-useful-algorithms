Cell[TextData[{
 "The components ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["t", "1"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f53b78c2-a9ff-497b-8cdb-da1adabbb7da"],
 "and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["t", "2"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "bd12e018-9596-422c-96ae-45be94a07597"],
 " are given without the spin dependence since the coefficients ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["w", 
    RowBox[{"1", ",", "2"}]], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "541aae92-e161-4bc5-91ab-b7d678bcfe65"],
 " will cancel out their spins in the fraction part"
}], "Text",
 CellChangeTimes->{{3.85961682468314*^9, 3.8596168919120827`*^9}, {
  3.8596171083629827`*^9, 
  3.8596171089493437`*^9}},ExpressionUUID->"edf1bfac-e267-42a8-b6f0-\
e22bb93723b7"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"t1", "=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", "*", "I2"}]], "+", 
    FractionBox["1", 
     RowBox[{"2", "*", "I1"}]], "-", 
    RowBox[{"2", 
     FractionBox["1", 
      RowBox[{"2", "*", "I3"}]]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"t2", "=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", "*", "I2"}]], "-", 
    FractionBox["1", 
     RowBox[{"2", "*", "I1"}]]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"w1", "=", 
   RowBox[{"Sqrt", "[", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       FractionBox["t1", 
        RowBox[{"Sqrt", "[", 
         RowBox[{
          SuperscriptBox["t1", "2"], "-", 
          SuperscriptBox["t2", "2"]}], "]"}]], "+", "1"}], ")"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"w2", "=", 
   RowBox[{"Sqrt", "[", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       FractionBox["t1", 
        RowBox[{"Sqrt", "[", 
         RowBox[{
          SuperscriptBox["t1", "2"], "-", 
          SuperscriptBox["t2", "2"]}], "]"}]], "-", "1"}], ")"}]}], "]"}]}], 
  ";"}]}], "Input",
