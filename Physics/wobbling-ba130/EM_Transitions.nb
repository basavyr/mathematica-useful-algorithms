3fa7e4008032"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Q0", "[", "beta_", "]"}], ":=", 
   RowBox[{
    FractionBox["3", 
     SqrtBox[
      RowBox[{"5", "\[Pi]"}]]], 
    SuperscriptBox["R", "2"], "*", "Z", "*", "beta", 
    RowBox[{"(", 
     RowBox[{"1", "+", 
      RowBox[{"0.16", "beta"}]}], ")"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Q20", "[", 
    RowBox[{"beta_", ",", "gamma_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"Q0", "[", "beta", "]"}], "*", 
    RowBox[{"Cos", "[", 
     RowBox[{"gamma", "*", 
      FractionBox["\[Pi]", "180"]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Q22", "[", 
    RowBox[{"beta_", ",", "gamma_"}], "]"}], ":=", 
   RowBox[{
    FractionBox[
     RowBox[{"Q0", "[", "beta", "]"}], 
     SqrtBox["2"]], 
    RowBox[{"Sin", "[", 
     RowBox[{"gamma", "*", 
      FractionBox["\[Pi]", "180"]}], "]"}]}]}], ";"}]}], "Input",
