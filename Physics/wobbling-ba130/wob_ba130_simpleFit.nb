Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"enB1", "[", 
    RowBox[{"I_", ",", "I3_", ",", "omega_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     FractionBox["1", 
      RowBox[{"2", "*", "I3"}]], "*", "I", 
     RowBox[{"(", 
      RowBox[{"I", "+", "1"}], ")"}]}], "+", 
    RowBox[{"omega", "*", 
     FractionBox["1", "2"]}]}]}], ";"}], "\[IndentingNewLine]", 
