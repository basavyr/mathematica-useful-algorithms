Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"enB1", "[", 
    RowBox[{"I_", ",", "I3_", ",", "omega_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     FractionBox["1", 
      RowBox[{"2", "*", "I3"}]], "*", "I", 
     RowBox[{"(", 
      RowBox[{"I", "+", "1"}], ")"}]}], "+", 
    RowBox[{"omega", "*", 
     FractionBox["1", "2"]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"enB2", "[", 
    RowBox[{"I_", ",", "I3_", ",", "omega_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     FractionBox["1", 
      RowBox[{"2", "*", "I3"}]], "*", "I", 
     RowBox[{"(", 
      RowBox[{"I", "+", "1"}], ")"}]}], "+", 
    RowBox[{"omega", "*", 
     FractionBox["3", "2"]}]}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.859360767523687*^9, 3.859360795715406*^9}, {
