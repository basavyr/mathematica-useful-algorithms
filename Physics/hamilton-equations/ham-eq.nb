 RowBox[{
  RowBox[{
   RowBox[{"ham", "[", 
    RowBox[{"n_", ",", "q_", ",", "qd_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"4", 
     SuperscriptBox["qd", "2"], "*", 
     RowBox[{"Sin", "[", "q", "]"}]}], "-", 
    RowBox[{"Exp", "[", "q", "]"}], "+", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"n", "+", 
       FractionBox["1", "2"]}], ")"}], "*", "\[Pi]", "*", 
     RowBox[{"Sqrt", "[", "qd", "]"}]}]}]}], ";", " ", 
