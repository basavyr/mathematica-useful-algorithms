Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "test", " ", "function", " ", "of", " ", "the", " ", "Hamiltonian"}], 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"ham", "[", 
      RowBox[{"n_", ",", "q_", ",", "qd_"}], "]"}], ":=", 
     RowBox[{
      RowBox[{"4", 
       SuperscriptBox["qd", "2"], "*", 
       RowBox[{"Sin", "[", "q", "]"}]}], "-", 
      RowBox[{"Exp", "[", "q", "]"}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"n", "+", 
         FractionBox["1", "2"]}], ")"}], "*", "\[Pi]", "*", 
       RowBox[{"Sqrt", "[", "qd", "]"}]}]}]}], ";"}], " ", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"dhamq", "[", 
      RowBox[{"n_", ",", "q_", ",", "qd_"}], "]"}], ":=", 
     RowBox[{"D", "[", 
      RowBox[{
       RowBox[{"ham", "[", 
        RowBox[{"1", ",", "q", ",", "qd"}], "]"}], ",", "q"}], "]"}]}], ";"}],
    "\[IndentingNewLine]", 
