   RowBox[{"hydroMOI", "[", 
    RowBox[{"I0_", ",", "k_", ",", "gamma_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["4", "3"], "*", "I0", "*", 
    SuperscriptBox[
     RowBox[{"Sin", "[", 
      RowBox[{
       RowBox[{"gamma", "*", 
        FractionBox["\[Pi]", "180"]}], "-", 
       RowBox[{
        FractionBox["2", "3"], "\[Pi]", "*", "k"}]}], "]"}], "2"]}]}], 
