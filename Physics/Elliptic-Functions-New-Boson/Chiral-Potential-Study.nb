     RowBox[{"v0term", "[", 
      RowBox[{"I", ",", "thetadeg", ",", "mois"}], "]"}], "*", 
     RowBox[{"cn", "[", 
      RowBox[{"q", ",", 
       RowBox[{"kterm", "[", 
        RowBox[{"I", ",", "thetadeg", ",", "mois"}], "]"}]}], "]"}], "*", 
     RowBox[{"dn", "[", 
      RowBox[{"q", ",", 
       RowBox[{"kterm", "[", 
