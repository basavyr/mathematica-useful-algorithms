Cell["Elliptic terms", "Item",
 CellChangeTimes->{{3.8860388324460993`*^9, 
  3.8860388359820747`*^9}},ExpressionUUID->"7e925e12-3cf5-434d-97b0-\
0049270ef308"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Aterm", "[", 
    RowBox[{"I_", ",", "thetadeg_", ",", "mois_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"A", "[", 
      RowBox[{"2", ",", "mois"}], "]"}], 
     RowBox[{"(", 
      RowBox[{"1", "-", 
       FractionBox[
        RowBox[{"j", "[", 
         RowBox[{"2", ",", "thetadeg"}], "]"}], "I"]}], ")"}]}], "-", 
    RowBox[{"A", "[", 
     RowBox[{"1", ",", "mois"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"uterm", "[", 
    RowBox[{"I_", ",", "thetadeg_", ",", "mois_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"A", "[", 
       RowBox[{"3", ",", "mois"}], "]"}], "-", 
      RowBox[{"A", "[", 
       RowBox[{"1", ",", "mois"}], "]"}]}], ")"}], "/", 
    RowBox[{"Aterm", "[", 
     RowBox[{"I", ",", "thetadeg", ",", "mois"}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"v0term", "[", 
    RowBox[{"I_", ",", "thetadeg_", ",", "mois_"}], "]"}], ":=", 
   RowBox[{"-", 
    FractionBox[
     RowBox[{"(", 
      RowBox[{
       RowBox[{"A", "[", 
        RowBox[{"1", ",", "mois"}], "]"}], "*", 
       RowBox[{"j", "[", 
        RowBox[{"1", ",", "thetadeg"}], "]"}]}], ")"}], 
     RowBox[{"Aterm", "[", 
      RowBox[{"I", ",", "thetadeg", ",", "mois"}], "]"}]]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"kterm", "[", 
    RowBox[{"I_", ",", "thetadeg_", ",", "mois_"}], "]"}], ":=", 
   RowBox[{"Sqrt", "[", 
    RowBox[{"Abs", "[", 
     RowBox[{"uterm", "[", 
      RowBox[{"I", ",", "thetadeg", ",", "mois"}], "]"}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"phi", "[", 
    RowBox[{"q_", ",", "k_"}], "]"}], ":=", 
   RowBox[{"JacobiAmplitude", "[", 
    RowBox[{"q", ",", 
     SuperscriptBox["k", "2"]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"sn", "[", 
    RowBox[{"q_", ",", "k_"}], "]"}], ":=", 
   RowBox[{"Sin", "[", 
    RowBox[{"phi", "[", 
     RowBox[{"q", ",", "k"}], "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"cn", "[", 
    RowBox[{"q_", ",", "k_"}], "]"}], ":=", 
   RowBox[{"Cos", "[", 
    RowBox[{"phi", "[", 
     RowBox[{"q", ",", "k"}], "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"dn", "[", 
    RowBox[{"q_", ",", "k_"}], "]"}], ":=", 
   SqrtBox[
    RowBox[{"1", "-", 
     RowBox[{
      SuperscriptBox["k", "2"], 
      SuperscriptBox[
       RowBox[{"sn", "[", 
        RowBox[{"q", ",", "k"}], "]"}], "2"]}]}]]}], ";"}]}], "Input",
