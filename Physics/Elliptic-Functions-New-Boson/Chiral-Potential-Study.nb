    RowBox[{"(", 
     RowBox[{
      RowBox[{"V", "[", 
       RowBox[{"q", ",", "I", ",", "thetadeg", ",", "mois"}], "]"}], "+", 
      RowBox[{"V", "[", 
       RowBox[{"q", ",", "I", ",", 
        RowBox[{"thetadeg", "+", "180"}], ",", "mois"}], "]"}]}], ")"}]}]}], 
